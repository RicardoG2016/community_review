<div class="album text-muted col-md-12 row"> 
  <% @post.each do |p| %>
    <div class="posts col-md-7">
      <%= link_to p  do %>
        <%= image_tag p.image.url(:small) %>
      <% end %>
      <br>
      <%= link_to p.title, p, class: "post-title" %>
      <br>
      <br>
      <p class="card-text post-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
      <hr>
      <%= link_to like_post_path(p), :method => :get do %>
        <i class="material-icons thumb">thumb_up</i>
        <span><%= p.get_upvotes.size %></span>
      <% end %>
      <%= link_to dislike_post_path(p), :method => :get do %>
        <i class="material-icons thumb">thumb_down</i>
        <span><%= p.get_downvotes.size %></span>
      <% end %>
      <br>
    </div>
  <% end %>
</div>